<form action="/albums/<%=album.id%>?_method=PUT" method="POST">
    <div class="mb-3">
      <label for="name" class="form-label">Album Name</label>
      <input type="text" class="form-control" id="name" aria-describedby="emailHelp" name='name' value="<%= album.name %>">
    </div>
    <div class="mb-3">
        <label for="artist" class="form-s">Artist</label>
        <input type="text" class="form-control" id="artist" aria-describedby="emailHelp" name='artist' value="<%= album.artist %>">
    </div>
    <div class="mb-3">
        <label for="songCount" class="form-label">Song Count</label>
        <input type="text" class="form-control" id="songCount" aria-describedby="emailHelp" name='songCount' value="<%= album.songCount %>">
    </div>
    <div class="mb-3">
        <label for="year" class="form-label">Year</label>
        <input type="text" class="form-control" id="year" aria-describedby="emailHelp" name='year' value="<%= album.year %>">
        <button type="submit" class="btn btn-primary">Update</button>
    </div>
  </form>